#include <stdio.h>

int main()
{
    int arr[] = {1, 2, 3, 4, 5};
    int size = sizeof(arr) / sizeof(arr[0]);
    printf("Forward traversal:\n");
    for (int i = 0; i < size; i++)
    {
        printf("%d ", arr[i]);
    }
    printf("\nBackward traversal:\n");
    for (int i = size - 1; i >= 0; i--)
        {
        printf("%d ", arr[i]);
    }

    return 0;
}
